{
    "name": "Web Cache",
    "version": "0.0.3",
    "manifest_version": 2,
    "description": "Open a cached version of the current tab",
    "icons": {
        "16": "images/icon-16.png",
        "24": "images/icon-24.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "scripts/chromereload.js",
            "scripts/background.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "32": "images/icon-32.png",
            "64": "images/icon-64.png"
        },
        "default_title": "web cache",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": [
        "tabs"
    ]
}